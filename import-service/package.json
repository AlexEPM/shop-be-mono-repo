{
  "name": "import-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "useDotenv": true,
  "scripts": {
    "test": "jest",
    "swagger": "sls generate-swagger",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:importProductsFile": "sls deploy function -f importProductsFile",
    "deploy:importFileParser": "sls deploy function -f importFileParser"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/s3-request-presigner": "^3.287.0",
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.2.6",
    "@middy/http-json-body-parser": "^3.4.0",
    "csv-parse": "^5.3.6"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "dotenv": "^16.0.3",
    "esbuild": "^0.14.11",
    "jest": "^29.5.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.12.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-esbuild": "^1.23.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
